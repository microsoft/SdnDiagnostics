<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<config>
		<add key="repositoryPath" value=".\packages" />
	</config>
	<packageSources>
		<!-- Do not define more than one package feed as may introduce supply chain vulnerability. -->
		<!-- Packages defined in packages.config must reside within the single package source -->
		<clear /> <!-- ensure only the sources defined below are used -->
		<add key = "PowerShellGallery" value="https://www.powershellgallery.com/api/v2" />
	</packageSources>
</configuration>
